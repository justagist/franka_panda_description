<?xml version='1.0'?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="panda">
  <xacro:macro name="panda_arm" params="arm_id:='panda' description_pkg:='franka_panda_description' connected_to:='' in_gazebo='false' xyz:='0 0 0' rpy:='0 0 0'">
    <!-- If using custom frame with ${connected_to} param, make sure 'world' frame exists in its TF tree -->
    <xacro:if value="${connected_to == ''}">
      <link name="world" />
      <xacro:property name="base_frame_name" value="world"/>
    </xacro:if>
    <xacro:unless value="${connected_to == ''}">
      <xacro:property name="base_frame_name" value="${connected_to}"/>
    </xacro:unless>
    <joint name="${arm_id}_to_${base_frame_name}" type="fixed">
      <parent link="${base_frame_name}"/>
      <child link="${arm_id}_link0"/>
      <origin rpy="${rpy}" xyz="${xyz}"/>
      <xacro:unless value="${in_gazebo == 'false'}">
        <dynamics damping="1.0"/>
      </xacro:unless>
    </joint>
    <link name="${arm_id}_link0">
      <visual>
        <geometry>
          <mesh filename="package://${description_pkg}/meshes/visual/link0.dae"/>
        </geometry>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://${description_pkg}/meshes/collision/link0.stl"/>
        </geometry>
      </collision>
      <xacro:unless value="${in_gazebo == 'false'}">
        <inertial>
          <origin xyz="0 0 0" rpy="0 0 0"/>
          <mass value="3.06"/>
          <inertia ixx="0.3" ixy="0.0" ixz="0.0" iyy="0.3" iyz="0.0" izz="0.3"/>
        </inertial>
      </xacro:unless>
    </link>
    <link name="${arm_id}_link1">
      <visual>
        <geometry>
          <mesh filename="package://${description_pkg}/meshes/visual/link1.dae"/>
        </geometry>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://${description_pkg}/meshes/collision/link1.stl"/>
        </geometry>
      </collision>
      <inertial>
        <origin xyz="3.875e-03 2.081e-03 -0.1750" rpy="0 0 0"/>
        <mass value="4.970684"/>
        <inertia ixx="7.0337e-01" ixy="-1.3900e-04" ixz="6.7720e-03" iyy="7.0661e-01" iyz="1.9169e-02" izz="9.1170e-03"/>
      </inertial>
    </link>
    <joint name="${arm_id}_joint1" type="revolute">
      <safety_controller k_position="100.0" k_velocity="40.0" soft_lower_limit="-2.8973" soft_upper_limit="2.8973"/>
      <origin rpy="0 0 0" xyz="0 0 0.333"/>
      <parent link="${arm_id}_link0"/>
      <child link="${arm_id}_link1"/>
      <axis xyz="0 0 1"/>
      <limit effort="87" lower="-2.8973" upper="2.8973" velocity="2.1750"/>
      <dynamics damping="10.0" friction="5.0"/>
    </joint>
    <link name="${arm_id}_link2">
      <visual>
        <geometry>
          <mesh filename="package://${description_pkg}/meshes/visual/link2.dae"/>
        </geometry>
      </visual>
      <collision>
        <geometry>
