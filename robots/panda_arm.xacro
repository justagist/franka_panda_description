<?xml version='1.0'?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="panda">
  <xacro:macro name="panda_arm" params="arm_id:='panda' description_pkg:='franka_panda_description' connected_to:='' in_gazebo='false' xyz:='0 0 0' rpy:='0 0 0'">
    <!-- If using custom frame with ${connected_to} param, make sure 'world' frame exists in its TF tree -->
    <xacro:if value="${connected_to == ''}">
      <link name="world" />
      <xacro:property name="base_frame_name" value="world"/>
    </xacro:if>
    <xacro:unless value="${connected_to == ''}">
      <xacro:property name="base_frame_name" value="${connected_to}"/>
    </xacro:unless>
    <joint name="${arm_id}_to_${base_frame_name}" type="fixed">
      <parent link="${base_frame_name}"/>
      <child link="${arm_id}_link0"/>
      <origin rpy="${rpy}" xyz="${xyz}"/>
      <xacro:unless value="${in_gazebo == 'false'}">
        <dynamics damping="1.0"/>
      </xacro:unless>
    </joint>
    <link name="${arm_id}_link0">
      <visual>
        <geometry>
          <mesh filename="package://${description_pkg}/meshes/visual/link0.dae"/>
        </geometry>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://${description_pkg}/meshes/collision/link0.stl"/>
        </geometry>
      </collision>
      <xacro:unless value="${in_gazebo == 'false'}">
        <inertial>
          <origin rpy="0 0 0" xyz="-0.025566 -2.88e-05 0.057332"/>
          <mass value="2.92"/>
          <inertia ixx="0.00782229414331" ixy="-1.56191622996e-05" ixz="-0.00126005738123" iyy="0.0109027971813" iyz="1.08233858202e-05" izz="0.0102355503949"/>
